      <oldValue><![CDATA[@SQL=Select case when count(*)> 0 Then 'Y' else 'N'End 
From sccc_cash_clousureline ccl
	Join sccc_cash_clousure cc on cc.sccc_cash_clousure_id = ccl.sccc_cash_clousure_id
	join sccc_setup st on st.ad_org_id = cc.ad_org_id
	join sccc_payment_method pm on st.sccc_setup_id = pm.sccc_setup_id
Where  pm.em_sscrcc_detailed = 'Y'
	And pm.fin_paymentmethod_id = @fin_paymentmethod_id@
	And ccl.sccc_cash_clousure_id = @sccc_cash_clousure_id@]]></oldValue>
      <newValue><![CDATA[@SQL=SELECT COALESCE(pm.em_sscrcc_detailed,'N')
FROM sccc_cash_clousureline AS ccl
	JOIN sccc_cash_clousure AS cc ON cc.sccc_cash_clousure_id=ccl.sccc_cash_clousure_id
	JOIN sccc_payment_method AS pm ON (pm.sccc_setup_id=cc.sccc_setup_id AND pm.fin_paymentmethod_id=ccl.fin_paymentmethod_id)
WHERE ccl.sccc_cash_clousureline_id=@Sccc_Cash_Clousureline_ID@
	AND ccl.fin_paymentmethod_id=@FIN_Paymentmethod_ID@]]></newValue>
    </columnDataChange>
