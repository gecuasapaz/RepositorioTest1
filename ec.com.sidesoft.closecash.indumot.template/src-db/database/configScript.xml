    <columnDataChange tablename="AD_TABLE" columnname="HQLQUERY" pkRow="D56CF1065EF14D52ADAD2AAB0CB63EFC">
      <oldValue><![CDATA[select e.id, e.transactionDate, bp.name as businessPartner, 
gl.name as gLItem, fp.documentNo as finPayment, e.description, 
e.paymentAmount, e.depositAmount 
@selectClause@
from FIN_Finacc_Transaction e
left join e.businessPartner as bp
left join e.gLItem as gl
left join e.finPayment fp
@joinClause@
where @additional_filters@
and e.processed = 'Y'
and @whereclause@]]></oldValue>
      <newValue><![CDATA[select e.id, e.transactionDate, bp.name as businessPartner, 
gl.name as gLItem, fp.documentNo as finPayment, e.description, 
e.paymentAmount, e.depositAmount, e.sscccinReference as em_sscccin_reference 
@selectClause@
from FIN_Finacc_Transaction e
left join e.businessPartner as bp
left join e.gLItem as gl
left join e.finPayment fp
@joinClause@
where @additional_filters@
and e.processed = 'Y'
and @whereclause@]]></newValue>
    </columnDataChange>
